{
  "app": {
    "vhosts": [
      "localhost",
      "127.0.0.1"
    ],
    "plugins": {
      "hello-world": {
        "module": "/test/fixtures/testPlugin.js",
        "options": {
          "hello": "bonjour"
        }
      }
    }
  },

  "services": {
    "module": "mixdown-config-filesystem",
    "options": {
      "paths": 0
    }
  },

  "main": {
    "module": "mixdown-server",
    "options": {
      "listen": {
        "port": 8081,
        "type": "tcp",
        "path": "/var/run/mixdown.sock"
      }
    }
  }

}